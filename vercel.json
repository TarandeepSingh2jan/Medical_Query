{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "env": {
    "NEO4J_URI": "@neo4j_uri",
    "NEO4J_USER": "@neo4j_user", 
    "NEO4J_PASSWORD": "@neo4j_password",
    "OPENROUTER_API_KEY": "@openrouter_api_key"
  }
}
